#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(void) {

    int number[100] = {0}; // 設定一個元素數量為100的「int」陣列
    int amount = 0; // 輸入的數字數量，同時兼具陣列的元素位置
    for (amount = 0; amount < 100; amount += 1) {
        int flag = scanf("%d", &number[amount]); // 輸入數字
        // 輸入「EOF」時，代表輸入結束
        if (flag == EOF) {
            break;
        }
    }

    //反序印出
    for (int i = amount-1; i >= 1; i--)
        printf("%d ", number[i]);
    printf("%d\n", number[0]);

   

    return 0;

}
